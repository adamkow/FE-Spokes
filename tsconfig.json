{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.js",
    "**/*.jsx",
    ".expo/types/**/*.js",
    "expo-env.d.js",
    "components/EditScreenInfo.jsx",
    "app/modal.jsx",
    "app/+not-found.jsx",
    "app/+html.jsx",
    "app/_layout.jsx",
    "app/(tabs)/two.jsx",
    "app/index.jsx",
    "app/(tabs)/_layout.jsx",
    "app/signUp.jsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
